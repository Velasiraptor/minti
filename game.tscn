text_1 = "ЭЙ! ТЫ ХТО? ТЫ ТОРГАШ?"
txt_1_arm_idle = false
txt_1_arm_nice = true
text_2 = "А ЗАЧЕМ ЭТУ КАРОВЬЮ ГОЛОВУ НАЦЕПИЛ? ДУРАЧОК СОВСЕМ? НУ ЛАДНО, ТЕБЕ ВИДНЕЕ, ЧУДИЛА!"
text_3 = "Я ОТБИЛСЯ ОТ СВОИХ БРАТЬЕВ, Г'ЛОХА И Р'УКА, МЫ АХОТИЛИСЬ НА КАБАНОВ, А ОДИН ИЗ НИХ ПРИВЕЛ МЕНЯ СЮДА..."
text_4 = "ТЫ ПРИКИНЬ КАКОЙ Я ГОЛОДНЫЙ! УХ, Я БЫ ЦЕЛОГО БЫКА СЛОПАЛ ЩАС! ХАХА ДА ШУЧУ Я..  ХОТЯ Я БЫ РЕАЛЬНО СЛОПАЛ ЕСЛИ ЧЕСТНО. У ТЕБЯ ЕСТЬ ЧО ПЕРЕКУСИТЬ?"
text_good = "ООО, А ВОТ ЭТО САВСЕМ ДРУГОЕ ДЕЛО! НУ, БУДЬ ЗДОРОВ, ДУРАЧОК"
text_bad = "ФУ, ЗАЧЕМ МНЕ ЭТО? ПОКА"
