[node name="Knight_1" parent="Dialogues" instance=ExtResource("14_004et")]
position = Vector2(-318, -10)
text_1 = "Здравствуй, ты Минотавр, верно?"
text_2 = "Меня зовут Вильгельм, я прибыл сюда с особой целью, но я не могу тебе ее раскрыть. Ты не видел здесь других людей? Помимо меня."
txt_2_arm_idle = false
txt_2_arm_dis = true
text_3 = "Я понял. В любом случае я бы воспользовался услугами твоей лавки, Минотавр."
text_4 = "У тебя есть что-нибудь, что поможет мне набраться сил? Я еле стою на ногах после долгой битвы."
text_good = "То что нужно. Спасибо, мне пора идти."
text_bad = "Это не то..."
item_1 = "energy"

