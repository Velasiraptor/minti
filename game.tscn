[node name="Skeleton_5" parent="Dialogues" instance=ExtResource("14_004et")]
position = Vector2(-318, -2)
sprite_frames = SubResource("SpriteFrames_sl8oo")
text_1 = "Фух, ну и денек, да? Я думаю нам уже пора закругляться, здоровяк. Встретил я тут одного коротышку, славный малый."
text_2 = "Я ненароком распугал его друзей, когда заходил сюда, хехе... Ну да ладно, он вроде не обиделся. Насыщенная выдалась смена да? "
txt_2_arm_idle = false
txt_2_arm_nice = true
text_3 = "И не говори. Ну ладно, спасибо что уделил мне время. И слушал мои байки. Пора мне идти дальше гулять по просторам этого мира."
text_4 = "А тебе бы я настоял отдохнуть, ты это заслужил, предприниматель хаха. Дай мне еще сувенирчик на дорогу"
text_good = "Ну ладно, все, я пошел. Не забывай меня когда высоко взлетишь!"
skeleton = true

